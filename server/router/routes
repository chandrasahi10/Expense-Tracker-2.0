const express = require('express');
const path = require('path');
const createUser = require(path.join(__dirname,'..','controllers','createUser.js'));
const home = require(path.join(__dirname,'..','controllers','home.js'));


const router = express.Router();

router.get('/signup',createUser.viewSignUp);

router.post('/home',createUser.doSignUp);
router.get('/home',home.viewHome);



module.exports = router;
